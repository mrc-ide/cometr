{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "NimueRun",
    "type": "object",
    "properties": {
        "timeSeries": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "date": {
                        "type": "string",
                        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                    },
                    "deaths": {
                        "type": "number"
                    },
                    "counterfactualDeaths": {
                        "type": "number"
                    },
                    "currentInfections": {
                        "type": "number"
                    },
                    "counterfactualCurrentInfections": {
                        "type": "number"
                    },
                    "hospitalisations": {
                        "type": "number"
                    },
                    "counterfactualHospitalisations": {
                        "type": "number"
                    },
                    "criticalCare": {
                        "type": "number"
                    },
                    "counterfactualCriticalCare": {
                        "type": "number"
                    },
                    "reportedDeaths": {
                        "type": ["number", "null"]
                    },
                    "Rt": {
                        "type": "number"
                    },
                    "Reff": {
                        "type": "number"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "date", "deaths", "counterfactualDeaths",
                    "currentInfections", "counterfactualCurrentInfections",
                    "hospitalisations", "counterfactualHospitalisations",
                    "criticalCare", "counterfactualCriticalCare",
                    "reportedDeaths", "Rt", "Reff"
                ]
            }
        },
        "fullyVaccinatedPersons": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "age": {
                        "type": "string"
                    },
                    "year2021": {
                        "type": "number"
                    },
                    "year2022": {
                        "type": "number"
                    },
                    "remainder": {
                        "type": "number"
                    }
                },
                additionalProperties: false,
                "required": ["age", "year2021", "year2022", "remainder"]
            }
        },
        "cumulativePercentPopulationVaccinated": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "month": {
                        "type": "string"
                    },
                    "healthcareWorkers": {
                        "type": "number"
                    },
                    "over65": {
                        "type": "number"
                    },
                    "workingAge": {
                        "type": "number"
                    }
                },
                additionalProperties: false,
                "required": ["month", "healthcareWorkers", "over65",
                             "workingAge"]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "timeSeries", "fullyVaccinatedPersons",
        "cumulativePercentPopulationVaccinated"]
}
